<?xml version="1.0" encoding="utf-8"?>
<project>

	<meta title="SfxrTest" package="com.fserb.SfxrTest" version="1.0.0" company="fserb" />
	<app main="SfxrTest" path="bin" file="SfxrTest" swf-version="11" />

	<window fps="30" background="0xFAFAFA" />
